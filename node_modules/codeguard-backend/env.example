# Server Configuration
PORT=5002
NODE_ENV=production

# ML Service Configuration
ML_SERVICE_URL=https://ml-service-a9l2.onrender.com

# CORS Configuration
CORS_ORIGINS=https://your-frontend-domain.com , https://codeguard-ai.netlify.app

# Rate Limiting Configuration
